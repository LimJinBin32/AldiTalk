","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-7099d0cf-12fd-4bc6-ab77-e4d15d966b92 button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","\n","    </div>\n","  </div>\n"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"dataframe","variable_name":"ocr_en","summary":"{\n  \"name\": \"ocr_en\",\n  \"rows\": 395,\n  \"fields\": [\n    {\n      \"column\": \"frame_name\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 395,\n        \"samples\": [\n          \"frame_9300.jpg\",\n          \"frame_30270.jpg\",\n          \"frame_23250.jpg\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"transcript_en\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 395,\n        \"samples\": [\n          \"Think about how your mother would feel if you committed suicide. salat lock ot Michael. alos wet\",\n          \"Yes\",\n          \"I don't like to be maligned\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"start\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 350.2549387391433,\n        \"min\": 83.0,\n        \"max\": 1257.8,\n        \"num_unique_values\": 387,\n        \"samples\": [\n          1087.4,\n          675.8,\n          395.0\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"end\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 350.02542547206525,\n        \"min\": 85.0,\n        \"max\": 1259.8,\n        \"num_unique_values\": 395,\n        \"samples\": [\n          373.0,\n          1211.8,\n          931.0\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"transcript_zh\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 369,\n        \"samples\": [\n          \"\\u4f60\\u4f4f\\u7684\\u662f C \\u7b49\\u75c5\\u623f \\u653f\\u5e9c\\u6709\\u8865\\u8d34\\u4f60\\u591a\\u8fbe80%\",\n          \"\\u8981\\u4e0d\\u5c31\\u6a44\\u6984\\u6cb9\\u4ee3\\u66ff\\u725b\\u6cb9\",\n          \"\\u73b0\\u5728\\u6211\\u4eec\\u8bf7\\u6211\\u4eec\\u7684\\u8bc4\\u5224\\u6765\\u8bd5\\u770b\\u4f60\\u7684\\u83dc\\u80b4\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"}},"metadata":{},"execution_count":21}]},{"cell_type":"code","source":["# Great; create a function with all previous steps\n","def process_ocr_to_df(json_file_path, start_music, end_music, fps=25):\n","    \"\"\"\n","    Returns a df with start and end timestamps for a corresponding transcript in English and Chinese.\n","    Argument:\n","    json_file_path (str): Path to the JSON file.\n","    start_music (int): Frame number of the start of the opening music.\n","    end_music (int): Frame number of the end of the ending music.\n","    fps (int): Frames per second\n","    \"\"\"\n","    # Load the JSON file\n","    with open(json_file_path, \"r\", encoding=\"utf-8\") as file:\n","        ocr_json = json.load(file)\n","\n","    # Convert the JSON data into a DataFrame\n","    rows = []\n","    for frame, texts in ocr_json.items():\n","        for text in texts:\n","            rows.append({\"frame_name\": frame, \"transcript\": text})\n","    ocr_df = pd.DataFrame(rows)\n","\n","    # Extract frame number as int\n","    ocr_df['frame_no'] = ocr_df['frame_name'].str.extract(r'frame_(\\d+)\\.jpg').astype(int)\n","\n","    # Sort rows by frame number first, fallback to index\n","    ocr_df = ocr_df.reset_index().sort_values(by=['frame_no', 'index'], ignore_index=True).drop('index', axis=1)\n","\n","    # Split into Chinese and English df\n","    ocr_zh = ocr_df[ocr_df['transcript'].apply(lambda x: bool(re.search(r'[\\u4e00-\\u9fff]', x)))].reset_index(drop=True)\n","    ocr_en = ocr_df[~ocr_df['transcript'].apply(lambda x: bool(re.search(r'[\\u4e00-\\u9fff]', x)))].reset_index(drop=True)\n","\n","    # Working with english df for now\n","\n","    # Quotations are annoying; remove them\n","    ocr_en['transcript'] = ocr_en['transcript'].str.replace(r'“|”|\"|‘|’', '', regex=True)\n","\n","    # Some frames are split into two rows; concatenate them\n","    ocr_en = ocr_en.groupby('frame_no').agg({\n","        'frame_name': 'first',\n","        'transcript': ' '.join\n","    }).reset_index()\n","\n","    # Delete duplicate transcripts\n","    first = ocr_en.drop_duplicates(subset='transcript', ignore_index=True, keep='first')\n","    last = ocr_en.drop_duplicates(subset='transcript', ignore_index=True, keep='last')\n","    ocr_en = pd.concat([first, last]).reset_index(drop=True).sort_values(by='frame_no', ignore_index=True)\n","\n","    # Take frames starting from 2000; exclude starting and ending music\n","    ocr_en = ocr_en[(ocr_en.frame_no>start_music) & (ocr_en.frame_no<end_music)]\n","\n","    # Convert frame_no into timestamps in seconds; videos are in 25 fps\n","    ocr_en['timestamp'] = ocr_en['frame_no'] / fps\n","\n","    # Get the start and end timestamps\n","    ocr_en['start'] = ocr_en['timestamp'].shift(1)\n","    ocr_en['end'] = ocr_en['timestamp']\n","    ocr_en.dropna(subset=['start'], inplace=True)\n","\n","    # Adjust the start and end timestamps to have pauses; like people pause before and after speaking\n","    ocr_en['start'] = ocr_en['start'] - 0.5\n","    ocr_en['end'] = ocr_en['end'] + 0.5\n","\n","    # Intentionally duplicated rows and unnecessary columns can be deleted now\n","    ocr_en = ocr_en.drop_duplicates(subset='transcript', ignore_index=True, keep='last')\n","    ocr_en.drop(['frame_no', 'timestamp'], axis=1, inplace=True)\n","\n","    # Do the same for ocr_zh\n","    ocr_zh['transcript'] = ocr_zh['transcript'].str.replace(r'“|”|\"|‘|’', '', regex=True)\n","    ocr_zh = ocr_zh.groupby('frame_no').agg({\n","        'frame_name': 'first',\n","        'transcript': ' '.join\n","    }).reset_index()\n","    ocr_zh.drop_duplicates(subset='transcript', ignore_index=True, inplace=True, keep='last')\n","\n","    # Add the column for Chinese transcripts with matching frame_name\n","    ocr_en['transcript_zh'] = ocr_en['frame_name'].map(ocr_zh.set_index('frame_name')['transcript'])\n","    ocr_en.rename(columns={'transcript': 'transcript_en'}, inplace=True)\n","\n","    return ocr_en"],"metadata":{"id":"-YKzVTQCHNGq"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Test the function on previous json file\n","df = process_ocr_to_df(json_file_path, 1875, 31500)\n","df.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":241},"id":"f1nh24KRWdYk","executionInfo":{"status":"ok","timestamp":1737684892927,"user_tz":-480,"elapsed":21,"user":{"displayName":"Min Phyo Thura","userId":"17625770619877039965"}},"outputId":"89c5be2d-5f94-4c38-8f7b-7e7419e0f229"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["       frame_name                                      transcript_en  start  \\\n","0  frame_2100.jpg                               Ladies and Gentlemen   83.0   \n","1  frame_2220.jpg  welcome to our Old Traditions, New Tastes cook...   84.2   \n","2  frame_2250.jpg       and thank you for supporting our contestants   89.0   \n","3  frame_2400.jpg  All our contestants are focused on preparing t...   90.2   \n","4  frame_2520.jpg  and we will find out who will be the winner ve...   96.2   \n","\n","     end       transcript_zh  \n","0   85.0              欢迎各位朋友  \n","1   89.8  今天来到我们传统新口味厨艺比赛的现场  \n","2   91.0            支持我们的参赛者  \n","3   97.0  看到各位厨师全神贯注准备他们的传统菜  \n","4  101.8            我们很快就会知道  "],"text/html":["\n","  <div id=\"df-a0719cee-eac8-4ede-8a7d-cfb768dfa219\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>frame_name</th>\n","      <th>transcript_en</th>\n","      <th>start</th>\n","      <th>end</th>\n","      <th>transcript_zh</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>0</th>\n","      <td>frame_2100.jpg</td>\n","      <td>Ladies and Gentlemen</td>\n","      <td>83.0</td>\n","      <td>85.0</td>\n","      <td>欢迎各位朋友</td>\n","    </tr>\n","    <tr>\n","      <th>1</th>\n","      <td>frame_2220.jpg</td>\n","      <td>welcome to our Old Traditions, New Tastes cook...</td>\n","      <td>84.2</td>\n","      <td>89.8</td>\n","      <td>今天来到我们传统新口味厨艺比赛的现场</td>\n","    </tr>\n","    <tr>\n","      <th>2</th>\n","      <td>frame_2250.jpg</td>\n","      <td>and thank you for supporting our contestants</td>\n","      <td>89.0</td>\n","      <td>91.0</td>\n","      <td>支持我们的参赛者</td>\n","    </tr>\n","    <tr>\n","      <th>3</th>\n","      <td>frame_2400.jpg</td>\n","      <td>All our contestants are focused on preparing t...</td>\n","      <td>90.2</td>\n","      <td>97.0</td>\n","      <td>看到各位厨师全神贯注准备他们的传统菜</td>\n","    </tr>\n","    <tr>\n","      <th>4</th>\n","      <td>frame_2520.jpg</td>\n","      <td>and we will find out who will be the winner ve...</td>\n","      <td>96.2</td>\n","      <td>101.8</td>\n","   