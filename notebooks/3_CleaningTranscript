JSON files\n","                    ocr_results_path = os.path.join(episode_path, \"ocr_results.json\")\n","                    status_path = os.path.join(episode_path, \"status.json\")\n","\n","                    # Check if required JSON files exist\n","                    if os.path.exists(ocr_results_path) and os.path.exists(status_path):\n","                        try:\n","                            # Process the OCR results\n","                            print(f\"    Found 'ocr_results.json': {ocr_results_path}\")\n","                            processed_ocr = process_ocr_to_df(ocr_results_path, start_time, end_time)\n","\n","                            # Create a unique output file path\n","                            cleaned_output_path = os.path.join(output_path, drama_folder)\n","                            os.makedirs(cleaned_output_path, exist_ok=True)  # Ensure drama-specific output folder exists\n","                            output_file_path = os.path.join(cleaned_output_path, f\"{episode_folder}_ocr_cleaned.json\")\n","\n","                            # Save the processed DataFrame as JSON\n","                            processed_ocr.to_json(output_file_path, orient='records', force_ascii=False, indent=4)\n","                            print(f\"    Processed data saved to: {output_file_path}\")\n","\n","                        except Exception as e:\n","                            # Handle and log any errors during processing\n","                            print(f\"    Error processing {episode_folder}: {e}\")\n","                            print(traceback.format_exc())\n","                    else:\n","                        print(f\"    Missing required files in: {episode_folder}\")\n","\n","# Define the root input directory and output directory\n","main_transcript_path = \"/content/drive/MyDrive/NYP/Year2/AI Engineering Project/AIEP [shared]/ocr\"\n","output_path = \"/content/drive/MyDrive/NYP/Year2/AI Engineering Project/AIEP [shared]\"\