                                          [key], {});\n","        if (!dataTable) return;\n","\n","        const docLinkHtml = 'Like what you see? Visit the ' +\n","          '<a target=\"_blank\" href=https://colab.research.google.com/notebooks/data_table.ipynb>data table notebook</a>'\n","          + ' to learn more about interactive tables.';\n","        element.innerHTML = '';\n","        dataTable['output_type'] = 'display_data';\n","        await google.colab.output.renderOutput(dataTable, element);\n","        const docLink = document.createElement('div');\n","        docLink.innerHTML = docLinkHtml;\n","        element.appendChild(docLink);\n","      }\n","    </script>\n","  </div>\n","\n","\n","<div id=\"df-ced4f5e6-a822-4059-846d-940f5bcee0ce\">\n","  <button class=\"colab-df-quickchart\" onclick=\"quickchart('df-ced4f5e6-a822-4059-846d-940f5bcee0ce')\"\n","            title=\"Suggest charts\"\n","            style=\"display:none;\">\n","\n","<svg xmlns=\"http://www.w3.org/2000/svg\" height=\"24px\"viewBox=\"0 0 24 24\"\n","     width=\"24px\">\n","    <g>\n","        <path d=\"M19 3H5c-1.1 0-2 .9-2 2v14c0 1.1.9 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zM9 17H7v-7h2v7zm4 0h-2V7h2v10zm4 0h-2v-4h2v4z\"/>\n","    </g>\n","</svg>\n","  </button>\n","\n","<style>\n","  .colab-df-quickchart {\n","      --bg-color: #E8F0FE;\n","      --fill-color: #1967D2;\n","      --hover-bg-color: #E2EBFA;\n","      --hover-fill-color: #174EA6;\n","      --disabled-fill-color: #AAA;\n","      --disabled-bg-color: #DDD;\n","  }\n","\n","  [theme=dark] .colab-df-quickchart {\n","      --bg-color: #3B4455;\n","      --fill-color: #D2E3FC;\n","      --hover-bg-color: #434B5C;\n","      --hover-fill-color: #FFFFFF;\n","      --disabled-bg-color: #3B4455;\n","      --disabled-fill-color: #666;\n","  }\n","\n","  .colab-df-quickchart {\n","    background-color: var(--bg-color);\n","    border: none;\n","    border-radius: 50%;\n","    cursor: pointer;\n","    display: none;\n","    fill: var(--fill-color);\n","    height: 32px;\n","    padding: 0;\n","    width: 32px;\n","  }\n","\n","  .colab-df-quickchart:hover {\n","    background-color: var(--hover-bg-color);\n","    box-shadow: 0 1px 2px rgba(60, 64, 67, 0.3), 0 1px 3px 1px rgba(60, 64, 67, 0.15);\n","    fill: var(--button-hover-fill-color);\n","  }\n","\n","  .colab-df-quickchart-complete:disabled,\n","  .colab-df-quickchart-complete:disabled:hover {\n","    background-color: var(--disabled-bg-color);\n","    fill: var(--disabled-fill-color);\n","    box-shadow: none;\n","  }\n","\n","  .colab-df-spinner {\n","    border: 2px solid var(--fill-color);\n","    border-color: transparent;\n","    border-bottom-color: var(--fill-color);\n","    animation:\n","      spin 1s steps(1) infinite;\n","  }\n","\n","  @keyframes spin {\n","    0% {\n","      border-color: transparent;\n","      border-bottom-color: var(--fill-color);\n","      border-left-color: var(--fill-color);\n","    }\n","    20% {\n","      border-color: transparent;\n","      border-left-color: var(--fill-color);\n","      border-top-color: var(--fill-color);\n","    }\n","    30% {\n","      border-color: transparent;\n","      border-left-color: var(--fill-color);\n","      border-top-color: var(--fill-color);\n","      border-right-color: var(--fill-color);\n","    }\n","    40% {\n","      border-color: transparent;\n","      border-right-color: var(--fill-color);\n","      border-top-color: var(--fill-color);\n","    }\n","    60% {\n","      border-color: transparent;\n","      border-right-color: var(--fill-color);\n","    }\n","    80% {\n","      border-color: transparent;\n","      border-right-color: var(--fill-color);\n","      border-bottom-color: var(--fill-color);\n","    }\n","    90% {\n","      border-color: transparent;\n","      border-bottom-color: var(--fill-color);\n","    }\n","  }\n","</style>\n","\n","  <script>\n","    async function quickchart(key) {\n","      const quickchartButtonEl =\n","        document.querySelector('#' + key + ' button');\n","      quickchartButtonEl.disabled = true;  // To prevent multiple clicks.\n","      quickchartButtonEl.classList.add('colab-df-spinner');\n","      try {\n","        const charts = await google.colab.kernel.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-ced4f5e6-a822-4059-846d-940f5bcee0ce button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","\n","    </div>\n","  </div>\n"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"dataframe","variable_name":"df","summary":"{\n  \"name\": \"df\",\n  \"rows\": 320,\n  \"fields\": [\n    {\n      \"column\": \"frame_name\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 320,\n        \"samples\": [\n          \"frame_18120.jpg\",\n          \"frame_23100.jpg\",\n          \"frame_4620.jpg\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"transcript_en\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 320,\n        \"samples\": [\n          \"and I have been working odd jobs all my life\",\n          \"Let me bring you to meet our MP\",\n          \"Chair ...\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"start\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 336.8257828029424,\n        \"min\": 78.2,\n        \"max\": 1244.6,\n        \"num_unique_values\": 320,\n        \"samples\": [\n          722.6,\n          921.8,\n          182.6\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"end\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 336.89226408964674,\n        \"min\": 80.2,\n        \"max\": 1247.8,\n        \"num_unique_values\": 320,\n        \"samples\": [\n          725.8,\n          925.0,\n          185.8\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"transcript_zh\",\n      \"properties\": {\n        \"dtype\": \"string\",\n        \"num_unique_values\": 313,\n        \"samples\": [\n          \"\\u90a3\\u8981\\u600e\\u4e48\\u529e\",\n          \"\\u4f60\\u4eca\\u5929\\u65e0\\u7f18\\u65e0\\u6545\\u6ca1\\u6765\\u505a\\u5de5\",\n          \"\\u6211\\u8981\\u7ee7\\u7eed\\u505a\"\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"}},"metadata":{},"execution_count":27}]},{"cell_type":"code","source":["# Redeine the function\n","def process_ocr_to_df(json_file_path, start_music, end_music, fps=25):\n","    \"\"\"\n","    Returns a df with start and end timestamps for a corresponding transcript in English and Chinese.\n","    Argument:\n","    json_file_path (str): Path to the JSON file.\n","    start_music (int): Frame number of the start of the opening music.\n","    end_music (int): Frame number of the end of the ending music.\n","    fps (int): Frames per second\n","    \"\"\"\n","    # Load the JSON file\n","    with open(json_file_path, \"r\", encoding=\"utf-8\") as file:\n","        ocr_json = json.load(file)\n","\n","    # Convert the JSON data into a DataFrame\n","    rows = []\n","    for frame, texts in ocr_json.items():\n","        for text in texts:\n","            rows.append({\"frame_name\": frame, \"transcript\": text})\n","    ocr_df = pd.DataFrame(rows)\n","\n","    # Extract frame number as int\n","    ocr_df['frame_no'] = ocr_df['frame_name'].str.extract(r'frame_(\\d+)\\.jpg').astype(int)\n","\n","    # Sort rows by frame number first, fallback to index\n","    ocr_df = ocr_df.reset_index().sort_values(by=['frame_no', 'index'], ignore_index=True).drop('index', axis=1)\n","\n","    # Split into Chinese and English df\n","    ocr_zh = ocr_df[ocr_df['transcript'].apply(lambda x: bool(re.search(r'[\\u4e00-\\u9fff]', x)))].reset_index(drop=True)\n","    ocr_en = ocr_df[~ocr_df['transcript'].apply(lambda x: bool(re.search(r'[\\u4e00-\\u9fff]', x)))].reset_index(drop=True)\n","\n","    # Working with english df for now\n","\n","    # Quotations are annoying; remove them\n","    ocr_en['transcript'] = ocr_en['transcript'].str.replace(r'“|”|\"|‘|’', '', regex=True)\n","\n","    # Some frames are split into two rows; concatenate them\n","    ocr_en = ocr_en.groupby('frame_no').agg({\n","        'frame_name': 'first',\n","        'transcript': ' '.join\n","    }).reset_index()\n","\n","    # Delete duplicate transcripts\n","    first = ocr_en.drop_duplicates(subset='transcript', ignore_index=True, keep='first')\n","    last = ocr_en.drop_duplicates(subset='transcript', ignore_index=True, keep='last')\n","    ocr_en = pd.concat([first, last]).reset_index(drop=True).sort_values(by='frame_no', ignore_index=True)\n","\n","    # Take frames starting from 2000; exclude starting and ending music\n","    ocr_en = ocr_en[(ocr_en.frame_no>start_music) & (ocr_en.frame_no<end_music)]\n","\n","    # Convert frame_no into timestamps in seconds; videos are in 25 fps\n","    ocr_en['timestamp'] = ocr_en['frame_no'] / fps\n","\n","    # Get the start and end timestamps\n","    ocr_en['start'] = ocr_en['timestamp'].shift(1)\n","    ocr_en['end'] = ocr_en['timestamp']\n","    ocr_en.dropna(subset=['start'], inplace=True)\n","\n","    # Adjust the start and end timestamps to have pauses; like people pause before and after speaking\n","    ocr_en['start'] = ocr_en['start'] - 1\n","    ocr_en['end'] = ocr_en['end'] + 1\n","\n","    # Intentionally duplicated rows and unnecessary columns can be deleted now\n","    ocr_en = ocr_en.drop_duplicates(subset='transcript', ignore_index=True, keep='last')\n","    ocr_en.drop(['frame_no', 'timestamp'], axis=1, inplace=True)\n","\n","    # Do the same for ocr_zh\n","    ocr_zh['transcript'] = ocr_zh['transcript'].str.replace(r'“|”|\"|‘|’', '', regex=True)\n","    ocr_zh = ocr_zh.groupby('frame_no').agg({\n","        'frame_name': 'first',\n","        'transcript': ' '.join\n","    }).reset_index()\n","    ocr_zh.drop_duplicates(subset='transcript', ignore_index=True, inplace=True, keep='last')\n","\n","    # Add the column for Chinese transcripts with matching frame_name\n","    ocr_en['transcript_zh'] = ocr_en['frame_name'].map(ocr_zh.set_index('frame_name')['transcript'])\n","    ocr_en.rename(columns={'transcript': 'transcript_en'}, inplace=True)\n","\n","    # Get the word counts\n","    ocr_en['eng_word_count'] = ocr_en['transcript_en'].apply(lambda x: len(x.split()))\n","    ocr_en[\"zh_word_count\"] = ocr_en[\"transcript_zh\"].apply(lambda x: len(str(x)) if pd.notnull(x) else 0)\n","\n","    # Remove those with big difference in word count\n","    ocr_en = ocr_en[abs(ocr_en['eng_word_count'] - ocr_en['zh_word_count']) <= 4]\n","\n","    # drop the unnecessary columns\n","    ocr_en.drop(['eng_word_count', 'zh_word_count'], axis=1, inplace=True)\n","\n","    # final drop of null and duplicates\n","    ocr_en.dropna(inplace=True)\n","    ocr_en = ocr_en.drop_duplicates(ignore_index=True, keep='last')\n","    assert ocr_en.isna().sum().sum() == 0, \"NaNs still exist\"\n","    assert ocr_en.duplicated().sum() == 0, \"Duplicates still exist\"\n","\n","    return ocr_en"],"metadata":{"id":"5T6jJLvRF7Xl"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Test it\n","df = process_ocr_to_df(EatAlready_ep2, 1875, 31500)\n","df.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"DT5SsdGqGbcv","executionInfo":{"status":"ok","timestamp":1737691190540,"user_tz":-480,"elapsed":408,"user":{"displayName":"Min Phyo Thura","userId":"17625770619877039965"}},"outputId":"6ee6dc9e-c354-4973-c61f-f199cb41a3f3"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["       frame_name                                      transcript_en  start  \\\n","0  frame_1980.jpg                         I really was a rice-waster   78.7   \n","1  frame_2040.jpg            always seemed to fail at everything ...   81.1   \n","2  frame_2130.jpg                                        brother ...   84.7   \n","3  frame_2160.jpg                                           Wake up!   85.9   \n","4  frame_2280.jpg  You never keep your promises and are always la...   90.7   \n","\n","    end  transcript_zh  \n","0  79.7        我真的是浪费米  \n","1  82.1         做什么都失败  \n","2  85.7              哥  \n","3  86.9           喂 醒醒  \n","4  91.7  你每次答应我的事情都没做到  "],"text/html":["\n","  <div id=\"df-c79d5148-584b-4a53-9346-7e59dc48f9c8\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>frame_name</th>\n","      <th>transcript_en</th>\n","      <th>start</th>\n","      <th>end</th>\n","      <th>transcript_zh</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>0</th>\n","      <td>frame_1980.jpg</td>\n","      <td>I really was a rice-waster</td>\n","      <td>78.7</td>\n","      <td>79.7</td>\n","      <td>我真的是浪费米</td>\n","    </tr>\n","    <tr>\n","      <th>